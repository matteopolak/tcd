{
	"categories": [
		{
			"title": "## 🚀 Features",
			"labels": [
				"feature"
			]
		},
		{
			"title": "## 🐛 Fixes",
			"labels": [
				"fix"
			]
		},
		{
			"title": "## 🧪 Tests",
			"labels": [
				"test"
			]
		}
	],
	"ignore_labels": [
		"ignore"
	],
	"sort": {
		"order": "ASC",
		"on_property": "mergedAt"
	},
	"template": "${{UNCATEGORIZED}}",
	"pr_template": "- **${{TITLE}}** (#${{NUMBER}}) by ${{AUTHOR}}",
	"empty_template": "- no changes",
	"max_tags_to_fetch": 200,
	"max_pull_requests": 200,
	"max_back_track_time_days": 365,
	"exclude_merge_branches": [],
	"tag_resolver": {
		"method": "semver",
		"filter": {
			"pattern": "v(.+)",
			"flags": "gu"
		}
	},
	"base_branches": [
		"dev"
	]
}